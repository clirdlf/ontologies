@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix o: <http://omeka.org/s/vocabs/o#> .
@prefix clir: <http://omeka.clir.org/ns/clir> .

<http://omeka.clir.org/ns/clir> a owl:Ontology ;
    dcterms:title "CLIR Grants Registry Ontology" ;
    dcterms:description "Ontology to describe grants submitted to CLIR in the Hidden Collections, Digitizing Hidden Collections, and Recordings at Risk programs" .

clir:Application a rdfs:Class ;
    rdfs:label "CLIR application for a regranting program" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

clir:institution a rdf:Property ;
    rdfs:label "Institution" ;
    rdfs:comment "Primary institution submitting the grant application" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

clir:primaryContact a rdf:Property ;
    rdfs:label "Primary Contact" ;
    rdfs:comment "Primary contact submitting the grant application" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

clir:identifier a rdf:Property ;
    rdfs:label "Identifier" ;
    rdfs:comment "Application identifier generated by the application system" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

clir:program a rdf:Property ;
    rdfs:label "Program" ;
    rdfs:comment "Program the application was submitted to (e.g. Hidden Collections, Digitizing Hidden Collections, Recordings at Risk)" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

clir:wasFunded a rdf:Property ;
    rdfs:label "Was Funded" ;
    rdfs:comment "Boolean if the application received funding" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

clir:applicantUnit a rdf:Property ;
    rdfs:label "Applicant Unit" ;
    rdfs:comment "Unit of the institution applying for the grant" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

clir:pi2Name a rdf:Property ;
    rdfs:label "PI2 Name" ;
    rdfs:comment "Name of second primary investigator" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

clir:pi2Institution a rdf:Property ;
    rdfs:label "PI2 Institution" ;
    rdfs:comment "Institution of the second primary investigator" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

clir:pi3Name a rdf:Property ;
    rdfs:label "PI3 Name" ;
    rdfs:comment "Name of third primary investigator" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

clir:pi3Institution a rdf:Property ;
    rdfs:label "PI3 Institution" ;
    rdfs:comment "Institution of the third primary investigator" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

clir:request a rdf:Property ;
    rdfs:label "Request" ;
    rdfs:comment "Amount the application budgeted for" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .
